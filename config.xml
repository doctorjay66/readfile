<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.myreadfile" version="1.0.0">
  <name>MyReadFile</name>
  <description>my read file</description>
  <author href="http://phonegap.com" email="support@phonegap.com">PhoneGap Team</author>
  <platform name="android" />
  <content src="index.html"/>
  <feature name="http://api.phonegap.com/1.0/file" />
  <feature name="http://api.phonegap.com/1.0/notification" />
  <plugin name="cordova-plugin-file" source="npm"/>  
  <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
</widget>
